datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AuthorInformation {
  biography String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  image     Json?
  name      String?
  updatedAt DateTime @updatedAt
}
